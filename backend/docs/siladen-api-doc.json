    "/api/jenis_pasien": {
      "get": {
        "tags": ["Jenis Pasien"],
        "summary": "Get All Jenis Pasien",
        "description": "Get All nama jenis pasien untuk kebutuhan input laporan",
        "responses": {
          "200": {
            "description": "Success Get All Jenis Pasien",
